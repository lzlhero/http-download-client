{
  "name": "HTTP Download Client",
  "description": "Execute shell download command from context menu.",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png"
    }
  },
  "default_locale": "en",
  "manifest_version": 2
}
